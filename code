    <script>
        const menu = [
  {
    id: 1,
    title: "Gyoza",
    category: "breakfast",
    price: 15.99,
    img: "https://images.immediate.co.uk/production/volatile/sites/30/2020/08/gyoza-2d840f9.jpg?resize=768,574",
    desc: `Pan-fried or steamed dumplings filled with a mixture of meat and vegetables`,
  },
  {
    id: 2,
    title: "Miso Soup",
