    desc: `A traditional Japanese soup made with miso paste and often containing tofu, seaweed, and green onions`,
  },
  {
    id: 3,
    title: "Yuzu Ade",
    category: "shakes",
    price: 6.99,
    img: "data:image/jpeg;base64,",
    desc: `A refreshing citrus drink made with yuzu fruit juice, soda water, and sometimes a touch of honey or sweetener`,
  },
  {
    id: 4,
    title: "Okonomiyaki",
    category: "breakfast",
    price: 20.99,
    img: "data:image/jpeg;base64,",
    desc: `A savory pancake made with cabbage, various toppings, and a special okonomiyaki sauce.`,
  },
  {
    id: 5,
    title: "Teriyaki",
    category: "lunch",
    price: 22.99,
    img: "data:image/jpeg;base64,",
    desc: `A cooking technique where ingredients are grilled or broiled with a sweet soy-based glaze.`,
  },
  {
    id: 6,
    title: "Yakitori",
    category: "lunch",
    price: 18.99,
    img: "data:image/jpeg;base64,",
    desc: `Skewered and grilled chicken or other meats, often served with a savory sauce`,
  },
  {
    id: 7,
    title: "Ramen",
    category: "breakfast",
    price: 8.99,
    img: "data:image/jpeg;base64,",
    desc: `Japanese noodle soup typically made with wheat noodles, broth, and toppings like pork, eggs, and seaweed.`,
  },
  {
    id: 8,
    title: "Tempura",
    category: "lunch",
    price: 12.99,
    img: "data:image/jpeg;base64,",
    desc: `Lightly battered and deep-fried vegetables, seafood, or meat`,
  },
  {
    id: 9,
    title: "Sashimi",
    category: "dinner",
    price: 16.99,
    img: "data:image/jpeg;base64,",
    desc: `Thin slices of fresh raw fish or seafood served with soy sauce and wasabi.`,
  },
  {
    id: 10,
    title: "Sushi",
    category: "dinner",
    price: 22.99,
    img: "data:image/jpeg;base64,",
    desc: `Vinegared rice topped with various ingredients, often including seafood, vegetables, and sometimes tropical fruits.`,
  },
];
// get parent element
const sectionCenter = document.querySelector(".section-center");
const btnContainer = document.querySelector(".btn-container");
// display all items when page loads
window.addEventListener("DOMContentLoaded", function () {
  diplayMenuItems(menu);
  displayMenuButtons();
});

function diplayMenuItems(menuItems) {
  let displayMenu = menuItems.map(function (item) {
    // console.log(item);

    return `<article class="menu-item">
          <img src=${item.img} alt=${item.title} class="photo" />
          <div class="item-info">
            <header>
              <h4>${item.title}</h4>
              <h4 class="price">$${item.price}</h4>
            </header>
            <p class="item-text">
              ${item.desc}
            </p>
          </div>
        </article>`;
  });
  displayMenu = displayMenu.join("");
  // console.log(displayMenu);

  sectionCenter.innerHTML = displayMenu;
}
function displayMenuButtons() {
  const categories = menu.reduce(
    function (values, item) {
      if (!values.includes(item.category)) {
        values.push(item.category);
      }
      return values;
    },
    ["all"]
  );
  const categoryBtns = categories
    .map(function (category) {
      return `<button type="button" class="filter-btn" data-id=${category}>
          ${category}
        </button>`;
    })
    .join("");

  btnContainer.innerHTML = categoryBtns;
  const filterBtns = btnContainer.querySelectorAll(".filter-btn");
  console.log(filterBtns);

  filterBtns.forEach(function (btn) {
    btn.addEventListener("click", function (e) {
      // console.log(e.currentTarget.dataset);
      const category = e.currentTarget.dataset.id;
      const menuCategory = menu.filter(function (menuItem) {
        // console.log(menuItem.category);
        if (menuItem.category === category) {
          return menuItem;
        }
      });
      if (category === "all") {
        diplayMenuItems(menu);
      } else {
        diplayMenuItems(menuCategory);
      }
    });
  });
}
